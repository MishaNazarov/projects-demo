{
  "name": "MCSCloud-web",
  "version": "0.0.1",
  "description": "React web-application for MCSCloud project",
  "main": "index.js",
  "repository": "git@gitlab.osinit.com:MCSTeam/MCSCloud-web.git",
  "license": "ISC",
  "engines": {
    "node": ">=6.9"
  },
  "scripts": {
    "dev": "webpack-dev-server --env dev-server --progress --colors --watch --inline --hot --host 0.0.0.0 --port 8080",
    "server": "cd build && node ./server.js",
    "server:debug": "cd build && node --inspect ./server.js",
    "build": "webpack",
    "build:server": "webpack --env server",
    "build:production": "webpack -p --env production",
    "build:dockerize": "REDIS_HOST=api.sst-cloud.com REDIS_PORT=6380 yarn build:production",
    "docker:run": "docker-compose -f docker-compose.yml up --build --force-recreate",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint": "eslint src",
    "autofix": "eslint src --fix"
  },
